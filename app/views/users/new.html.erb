<%= stylesheet_link_tag "login", media: "all" %>

<% flash.each do |type, msg| %>
  <div>
    <%= msg %>
  </div>
<% end %>

<h1>Sign Up</h1>
<%= form_for @user do |f|%>
  <div>
    <%= f.label :username%><br>
    <%= f.text_field :username%><br>
  </div>

  <div>
    <%= f.label :password%><br>
    <%= f.password_field :password%><br>
  </div>

  <div>
    <%= f.submit %>
  </div>
<%end%>
